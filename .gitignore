# Create a .gitignore file if not already present
touch .gitignore

# Add Next.js specific entries
echo "# Next.js
.next/
out/
node_modules/
.env.local
.env.development.local
.env.test.local
.env.production.local
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.pnpm-debug.log*
.vercel
" > .gitignore

# Commit and push
git add .gitignore
git commit -m "Add Next.js specific .gitignore"
git push